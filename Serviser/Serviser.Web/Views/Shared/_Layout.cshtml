<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" type="text/css" href="~/Content/calander.css">
    
    @* Includes Stylesheets defined in the stylesheet section of views. *@
    @RenderSection("stylesheets", false)

</head>
<body>

    @{ Html.RenderPartial("_NavbarPartial");}

    
    @RenderBody()

    @{Html.RenderPartial("_FooterPartial");}


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
